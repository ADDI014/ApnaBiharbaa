<% layout("layouts/boilerplate.ejs") %>
<body>
    <div class="row row-cols-lg-4 row-cols-md-3 row-cols-sm-1">
      <% for(let Allproducts of CulturalProductBihar) { %>
        <a href="/apnabihar/culturalproducts/<%= Allproducts._id %>" class="listing-link">
          <div class="card col">
            <img class="card-img-top" src="<%= Allproducts.images[0].path %>" alt="Card image cap" style="height: 20rem;">
            <div class="card-img-overlay"></div>
            <div class="card-body">
              <p class="card-text"><b><%= Allproducts.name %></b></p>
            </div>
          </div>
        </a>
      <% } %>
    </div>   
</body>
